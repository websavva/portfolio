{
  "name": "@websavva/portfolio-website",
  "version": "2.6.0",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": "22.x",
    "pnpm": "10.10.0"
  },
  "scripts": {
    "build": "nuxt build --dotenv=../../.env",
    "dev": "nuxt dev --dotenv=../../.env --no-fork",
    "dev:prepare": "nuxt prepare --dotenv=../../.env",
    "typecheck:server": "tsc --noEmit -p server",
    "typecheck:app": "tsc --noEmit",
    "typecheck": "pnpm typecheck:server && pnpm typecheck:app",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "dependencies": {
    "@nuxt/content": "^3.7.1",
    "nuxt": "4.1.3",
    "vue": "3.5.22",
    "@websavva/portfolio-core": "workspace:*",
    "@websavva/portfolio-cv": "workspace:*",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@nuxt/kit": "^4.1.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.7.1",
    "better-sqlite3": "^12.4.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint": "^9.35.0",
    "typescript": "^5.9.3",
    "bowser": "^2.12.1",
    "defu": "^6.1.4",
    "tailwindcss": "^4.1.14",
    "motion-v": "^1.7.2",
    "zod": "^3.25.76"
  }
}
