{
  "name": "@websavva/portfolio-monorepo",
  "version": "1.1.2",
  "private": true,
  "packageManager": "pnpm@10.10.0",
  "type": "module",
  "pnpm": {
    "neverBuiltDependencies": []
  },
  "engines": {
    "node": "22.x",
    "pnpm": "10.10.0"
  },
  "scripts": {
    "website:build": "pnpm --filter @websavva/portfolio-website build",
    "website:dev": "pnpm --filter @websavva/portfolio-website dev",
    "website:dev:prepare": "pnpm --filter @websavva/portfolio-website dev:prepare",
    "cv:build": "pnpm --filter @websavva/portfolio-cv build",
    "cv:dev": "pnpm --filter @websavva/portfolio-cv dev",
    "cv:dev:prepare": "pnpm --filter @websavva/portfolio-cv dev:prepare",
    "core:dev:prepare": "pnpm --filter @websavva/portfolio-core dev:prepare"
  }
}
